#include "<df>/dragonfruit.h"
#include "<ll>/rtaisix/aisix.h"

fn private Usage { -- }
	"usage: chown owner file\n" STDERR FPrintf
end

fn Main { ... -- ret }
	0 ret!

	if (argc@ 3 ~=)
		Usage
		1 Exit
	end

	[2]argv@ [1]argv@ atoi Chown ret!

	if (ret@ iserr)
		ret@ abs ret!
		[ret@]ErrorNames@ [2]argv@ "chown: %s: %s\n" Printf
		1 ret!
	end
end