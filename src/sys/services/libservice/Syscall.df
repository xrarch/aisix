#include "<df>/dragonfruit.h"
#include "<ll>/rtaisix/aisix.h"

procedure Getc { -- c }
	asm "

	li r0, 3
	sys 0
	pushv r5, r0

	" c!
end

procedure Putc { c -- }
	c@ asm "

	popv r5, r1
	li r0, 0
	sys 0

	"
end

procedure AddService { name -- ok }
	name@ dup strlen asm "

	popv r5, r2
	popv r5, r1
	li r0, 5
	sys 0
	pushv r5, r0

	" ok!
end